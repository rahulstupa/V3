<body>
	<form id="update-matchid">
	
		<button onclick=plusAscore() class="btn btn-secondary btn-sm" >Point Trigger</button>
		<button onclick=plusBscore() class="btn btn-secondary btn-sm">Set Trigger</button>

		
	</form>
	<script>
		const setTrigger = () => {
			nodecg.sendMessage("triggerSetHeatmaps")
		}
		const matchTrigger = () => {
			nodecg.sendMessage("triggerMatchHeatmaps")
		}
		const pointTrigger = () => {
			nodecg.sendMessage('triggerRallyGraphic')
		}
		const pitchTrigger = () => {
			nodecg.sendMessage('showPitches', data, coordinates)
		}


		
	</script>
